<h1>Tasks</h1>
<table>
    <thead>
      <tr>
      <th>ID</th>
      <th>NAME</th>
      <th>STATUS</th>
      <th>DUE</th>
      <th>LABEL</th>
      </tr>
    </thead>
    <tbody>
      <% @tasks.each do |t| %>
        <tr>
        <td><%=t.id %></td>
        <td><%=t.name %></td>
        <td><%=t.status %></td>
        <td><%=t.duedate %></td>
        <td><%=t.label %></td>
        </tr>
      <% end %>
    </tbody>
</table>
